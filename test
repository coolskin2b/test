                    @setvar! jaseowns_BsPetOne 1
                endif
            else 
                @ignore jaseowns_BsPetOne
            endif
        endif
        
        if castSummonCreatureCount > 0
            if dead jaseowns_SummonCreaturePetOne or not find jaseowns_SummonCreaturePetOne ground -1 -1 18
                if followers < 5
